        "sampling_steps": {
          "id": "nb_steps"
        },
        "negative_prompt": {
          "id": "reverse_prompt"
        },
        "cfg_scale": {
          "id": "guidance_scale"
        },
        "highres._fix": {
          "id": "enable_hr"
        },
        "resize_mode": {
          "id": "resize_options"
        }
      },
      "inputs": "auto",
      "outputs": [
        {
          "label": "Result",
          "type": "image"
        }, {
          "label": "JSON metadata",
          "type": "json"
        }, {
          "label": "HTML metadata",
          "type": "html"
        }
      ]
    },
    {
