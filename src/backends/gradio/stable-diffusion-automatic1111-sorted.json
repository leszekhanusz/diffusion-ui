        },
        "highres._fix": {
          "id": "enable_hr"
        },
        "resize_mode": {
          "id": "resize_options"
        },
        "mask_mode": {
          "value": "Upload mask",
          "visible": false
        },
        "masking_mode": {
          "id": "inpainting_mask_invert",
          "visible": true
        },
        "label": {
          "value": 1
        }
      },
      "inputs": "auto",
      "outputs": [
        {
          "label": "Result",
          "type": "image"
        }, {
          "label": "JSON metadata",
          "type": "json"
        }, {
          "label": "HTML metadata",
          "type": "html"
        }
      ]
    },
    {
