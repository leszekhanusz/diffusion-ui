  "type": "gradio",
  "common_inputs": [
      {
        "label": "Prompt",
        "id": "prompt",
        "description": "Description of the image to generate",
        "type": "text",
        "default": "Bouquet of roses"
      }, {
        "label": "Reverse prompt",
        "id": "reverse_prompt",
        "description": "What not to generate",
        "type": "text",
        "default": ""
      }
  ],
  "functions": [
    {
      "id": "txt2img",
      "mode": "txt2img",
      "label": "Text to Image",
      "fn_index": {
        "conditions": {
          "js": "submit",
          "trigger": "click"
        }
      },
      "handle_output": "automatic1111",
      "layout": [
        {
          "type": "container",
          "id": "sampling_options",
          "label": "Sampling",
          "components": [
            {
              "type": "input",
              "id": "batch_count"
            },
            {
              "type": "input",
              "id": "batch_size"
            },
            {
              "type": "input",
              "id": "sampling_method"
            },
            {
              "type": "input",
              "id": "nb_steps"
            }
          ]
        },
        {
          "type": "container",
          "id": "prompt_options",
          "label": "Prompt options",
          "components": [
            {
              "type": "input",
              "id": "reverse_prompt"
            },
            {
              "type": "input",
              "id": "guidance_scale"
            }
          ]
        },
        {
          "type": "container",
          "id": "image_options",
          "label": "Image size",
          "components": [
            {
              "type": "input",
              "id": "width"
            },
            {
              "type": "input",
              "id": "height"
            }
          ]
        },
        {
          "type": "input",
          "id": "seed"
        },
        {
          "type": "container",
          "id": "extra_options",
          "label": "Extra",
          "components": [
            {
              "type": "input",
              "id": "restore_faces"
            },
            {
              "type": "input",
              "id": "tiling"
            },
            {
              "type": "input",
              "id": "enable_hr"
            },
            {
              "type": "input",
              "id": "strength"
            }
          ]
        }
      ],
      "auto_inputs": {
        "denoising_strength": {
          "id": "strength",
          "visible": {
            "input_id": "enable_hr",
            "condition": "===",
            "value": true
          }
        },
        "sampling_steps": {
          "id": "nb_steps"
        },
        "negative_prompt": {
          "id": "reverse_prompt"
        },
        "cfg_scale": {
          "id": "guidance_scale"
        },
        "highres._fix": {
          "id": "enable_hr"
        }
      },
      "inputs": "auto",
      "outputs": [
        {
          "label": "Result",
          "type": "image"
        }, {
          "label": "JSON metadata",
          "type": "json"
        }, {
          "label": "HTML metadata",
          "type": "html"
        }
      ]
    },
